// Use this file as a starting point for your project"s .eslintrc.
// Copy this file, and add rule overrides as needed.
{
  "extends": "airbnb",
  "rules": {
    "padded-blocks": "off",
    "no-multi-assign": "off",
    "prefer-destructuring": ["off", {
      "array": true,
      "object": true
    }, {
      "enforceForRenamedProperties": false
    }]
  }
}
